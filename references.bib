 title = {Gender differences in personality: a meta-analysis},
 keywords = {Adolescent;Adult;Aged;Anxiety;Extraversion, Psychological;Female;Humans;Internal-External Control;Male;Middle Aged;Observer Variation;Personality;Personality Inventory;Self Concept;Sex Factors},
