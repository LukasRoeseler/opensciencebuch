@article{Miyakawa.2020,
 abstract = {A reproducibility crisis is a situation where many scientific studies cannot be reproduced. Inappropriate practices of science, such as HARKing, p-hacking, and selective reporting of positive results, have been suggested as causes of irreproducibility. In this editorial, I propose that a lack of raw data or data fabrication is another possible cause of irreproducibility.As an Editor-in-Chief of Molecular Brain, I have handled 180 manuscripts since early 2017 and have made 41 editorial decisions categorized as {\textquotedbl}Revise before review,{\textquotedbl} requesting that the authors provide raw data. Surprisingly, among those 41 manuscripts, 21 were withdrawn without providing raw data, indicating that requiring raw data drove away more than half of the manuscripts. I rejected 19 out of the remaining 20 manuscripts because of insufficient raw data. Thus, more than 97{\%} of the 41 manuscripts did not present the raw data supporting their results when requested by an editor, suggesting a possibility that the raw data did not exist from the beginning, at least in some portions of these cases.Considering that any scientific study should be based on raw data, and that data storage space should no longer be a challenge, journals, in principle, should try to have their authors publicize raw data in a public database or journal site upon the publication of the paper to increase reproducibility of the published results and to increase public trust in science.},
 author = {Miyakawa, Tsuyoshi},
 year = {2020},
 title = {No raw data, no science: another possible source of the reproducibility crisis},
 keywords = {Data fabrication;Misconduct;open data;open science;Raw data;Reproducibility},
 pages = {24},
 volume = {13},
 number = {1},
 journal = {Molecular brain},
 doi = {10.1186/s13041-020-0552-2},
