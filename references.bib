@article{Rashotte.2012,
 abstract = {Context effects on tempo and pleasantness judgments of different tempos were demonstrated in three experiments using Beatles songs. In Experiments 1 and 2, we explored how listening to versions of the same song that were played at different tempos affected tempo and pleasantness ratings. In both experiments, contrast effects were found on judgments of tempo, with target tempos rated faster when context tempos were slow than when they were fast. In both experiments, we also showed that the peak of the pleasantness rating function shifted toward the values of the context tempos, reflecting disordinal context effects on pleasantness relationships. Familiarity with the songs did not moderate these effects, and shifts in tempo ratings did not correlate with shifts in most pleasant target tempos when context was manipulated within subjects. In Experiment 3, we examined how manipulations of context tempos for one song affected judgments of the same song as compared with judgments of other more or less similar songs. For tempo ratings, contrast effects transferred to ratings of a similar song, but for pleasantness ratings, assimilative shifts of ideals were found only for the same song and not for similar songs. This pattern of results was supportive of independent bases for the two context effects.},
 author = {Rashotte, Matthew A. and Wedell, Douglas H.},
 year = {2012},
 title = {Context effects on tempo and pleasantness judgments for Beatles songs},
 keywords = {Association;Auditory Perception;Choice Behavior;Humans;Judgment;Music;Psychoacoustics;Recognition (Psychology);Time Perception},
 pages = {575--599},
 volume = {74},
 number = {3},
 issn = {1943-3921},
 journal = {Attention, perception {\&} psychophysics},
 doi = {10.3758/s13414-011-0255-y},
 file = {Rashotte-Wedell2012{\_}Article{\_}ContextEffectsOnTempoAndPleasa:Attachments/Rashotte-Wedell2012{\_}Article{\_}ContextEffectsOnTempoAndPleasa.pdf:application/pdf}
}


@article{Rashotte.2014,
 abstract = {In two experiments, we investigated context effects on tempo judgments for familiar and unfamiliar songs performed by popular artists. In Experiment 1, participants made comparative tempo judgments to a remembered standard for song clips drawn from either a slow or a fast context, created by manipulating the tempos of the same songs. Although both familiar and unfamiliar songs showed significant shifts in their points of subjective equality toward the tempo context values, more-familiar songs showed significantly reduced contextual bias. In Experiment 2, tempo pleasantness ratings showed significant context effects in which the ordering of tempos on the pleasantness scale differed across contexts, with the most pleasant tempo shifting toward the contextual values, an assimilation of ideal points. Once again, these effects were significant but reduced for the more-familiar songs. The moderating effects of song familiarity support a weak version of the absolute-tempo hypothesis, in which long-term memory for tempo reduces but does not eliminate contextual effects. Thus, although both relative and absolute tempo information appear to be encoded in memory, the absolute representation may be subject to rapid revision by recently experienced tempo-altered versions of the same song.},
 author = {Rashotte, Matthew A. and Wedell, Douglas H.},
 year = {2014},
 title = {Testing the absolute-tempo hypothesis: context effects for familiar and unfamiliar songs},
 keywords = {Adult;Auditory Perception/physiology;Humans;Music;Recognition (Psychology)/physiology;Time Perception/physiology;Young Adult},
 pages = {1302--1314},
 volume = {42},
 number = {8},
 issn = {1532-5946},
 journal = {Memory {\&} cognition},
 doi = {10.3758/s13421-014-0434-x},
 file = {Rashotte-Wedell2014{\_}Article{\_}TestingTheAbsolute-tempoHypoth:Attachments/Rashotte-Wedell2014{\_}Article{\_}TestingTheAbsolute-tempoHypoth.pdf:application/pdf}
}


