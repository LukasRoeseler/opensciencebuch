@article{Pohl.2003,
 abstract = {The cognitive process model {\textquotedbl}SARA{\textquotedbl} aims to explain the anchoring effect and hindsight bias by making detailed assumptions about the representation and alteration of item-specific knowledge. The model assumes that all processes, namely generating an estimate, encoding new information (i.e., the {\textquotedbl}anchor{\textquotedbl}), and reconstructing a previously generated estimate, are based on a probabilistic sampling process. Sampling probes long-term memory in order to retrieve information into working memory. Retrieval depends on the associative strength between this information and the currently active retrieval cues. Encoding the anchor may alter this associative pattern ({\textquotedbl}selective activation{\textquotedbl}) or the anchor may serve as a retrieval cue, thus directing memory search ({\textquotedbl}biased reconstruction{\textquotedbl}). Both processes lead to systematically changed retrieval probabilities, thus causing the anchoring effect or hindsight bias. The model is completely formalised and implemented as a computer program. A series of simulations demonstrates the power of SARA to reproduce empirical findings and to predict new ones.},
 author = {Pohl, Rudiger F. and Eisenhauer, Markus and Hardt, Oliver},
 year = {2003},
 title = {SARA: a cognitive process model to simulate the anchoring effect and hindsight bias},
 keywords = {Association;Cognition;Cues;Humans;Judgment;Memory, Short-Term;Mental Recall;Models, Psychological;Probability},
 pages = {337--356},
 volume = {11},
