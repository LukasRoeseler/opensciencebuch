@article{Mede.2020,
 abstract = {Several meta-analytical attempts to reproduce results of empirical research have failed in recent years, prompting scholars and news media to diagnose a {\textquotedbl}replication crisis{\textquotedbl} and voice concerns about science losing public credibility. Others, in contrast, hoped replication efforts could improve public confidence in science. Yet nationally representative evidence backing these concerns or hopes is scarce. We provide such evidence, conducting a secondary analysis of the German {\textquotedbl}Science Barometer{\textquotedbl} ({\textquotedbl}Wissenschaftsbarometer{\textquotedbl}) survey. We find that most Germans are not aware of the {\textquotedbl}replication crisis.{\textquotedbl} In addition, most interpret replication efforts as indicative of scientific quality control and science's self-correcting nature. However, supporters of the populist right-wing party AfD tend to believe that the {\textquotedbl}crisis{\textquotedbl} shows one cannot trust science, perhaps using it as an argument to discredit science. But for the majority of Germans, hopes about reputational benefits of the {\textquotedbl}replication crisis{\textquotedbl} for science seem more justified than concerns about detrimental effects.},
 author = {Mede, Niels G. and Sch{\"a}fer, Mike S. and Ziegler, Ricarda and Wei{\ss}kopf, Markus},
 year = {2020},
 title = {The {\textquotedbl}replication crisis{\textquotedbl} in the public eye: Germans' awareness and perceptions of the (ir)reproducibility of scientific research},
 pages = {963662520954370},
 journal = {Public understanding of science (Bristol, England)},
 doi = {10.1177/0963662520954370},
 file = {Mede-et-al{\_}Accepted-Manuscript{\_}Replication-Crisis:Attachments/Mede-et-al{\_}Accepted-Manuscript{\_}Replication-Crisis.pdf:application/pdf}
}


