 issn = {0956-7976},
 journal = {Psychological Science},
 doi = {10.1177/09567976221089599},
 file = {Spicer, Zhu et al 2022 - Perceptual and Cognitive Judgments Show:Attachments/Spicer, Zhu et al 2022 - Perceptual and Cognitive Judgments Show.pdf:application/pdf}
