 keywords = {Bayes Theorem;Humans;Periodicals as Topic/statistics {\&} numerical data;Reproducibility of Results;Research/statistics {\&} numerical data;Sample Size;Social Sciences/methods/statistics {\&} numerical data},
 pages = {637--644},
 volume = {2},
