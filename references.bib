 keywords = {Adolescent;Adult;Aged;Aged, 80 and over;Cross-Cultural Comparison;Decision making;Female;Humans;Male;Middle Aged;Psychological Theory;Reproducibility of Results;Risk;Risk-Taking;Young Adult},
