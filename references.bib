@article{Ashford.2019,
 abstract = {Previous research has found initial evidence that word choice impacts the perception and treatment of those with behavioral health disorders through explicit bias (i.e., stigma). A more robust picture of behavioral health disorder stigma should incorporate both explicit and implicit bias, rather than relying on only one form. The current study uses the Go/No-Go Association Task to calculate a d' (sensitivity) indexed score of automatic attitudes (i.e., implicit associations) to two terms, {\textquotedbl}addict{\textquotedbl} and {\textquotedbl}person with substance use disorder.{\textquotedbl} Participants have significantly more negative automatic attitudes (i.e., implicit bias) toward the term {\textquotedbl}addict{\textquotedbl} in isolation as well as when compared to {\textquotedbl}person with a substance use disorder.{\textquotedbl} Consistent with previous research on explicit bias, implicit bias does exist for terms commonly used in the behavioral health field. {\textquotedbl}Addict{\textquotedbl} should not be used in professional or lay settings. Additionally, these results constitute the second pilot study employed the Go/No-Go Association Task in this manner, suggesting it is a viable option for continued linguistic stigma related research.},
 author = {Ashford, Robert D. and Brown, Austin M. and Curtis, Brenda},
