 keywords = {Adult;Damaged self-esteem;Depression/complications/psychology;Discrepancies;Explicit self-esteem;Female;Fragile self-esteem;Humans;Implicit self-esteem;Male;Narcissistic personality disorder;Personality Disorders/complications/diagnosis/psychology;Personality Inventory;Psychiatric Status Rating Scales;Self Concept;Self Report;Surveys and Questionnaires;Young Adult},
 pages = {37--47},
 volume = {44},
 number = {1},
 issn = {1873-7943},
 journal = {Journal of behavior therapy and experimental psychiatry},
 doi = {10.1016/j.jbtep.2012.07.001},
 file = {vater2013:Attachments/vater2013.pdf:application/pdf}
