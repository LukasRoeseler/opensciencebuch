 pages = {995--1012},
 volume = {101},
 number = {7},
 issn = {0021-9010},
 journal = {The Journal of applied psychology},
 doi = {10.1037/apl0000096},
 file = {ContentServer (43):Attachments/ContentServer (43).pdf:application/pdf}
