@article{Lakens.2021b,
 abstract = {Because of the strong overreliance on p values in the scientific literature, some researchers have argued that we need to move beyond p values and embrace practical alternatives. When proposing alternatives to p values statisticians often commit the {\textquotedbl}statistician's fallacy,{\textquotedbl} whereby they declare which statistic researchers really {\textquotedbl}want to know.{\textquotedbl} Instead of telling researchers what they want to know, statisticians should teach researchers which questions they can ask. In some situations, the answer to the question they are most interested in will be the p value. As long as null-hypothesis tests have been criticized, researchers have suggested including minimum-effect tests and equivalence tests in our statistical toolbox, and these tests have the potential to greatly improve the questions researchers ask. If anyone believes p values affect the quality of scientific research, preventing the misinterpretation of p values by developing better evidence-based education and user-centered statistical software should be a top priority. Polarized discussions about which statistic scientists should use has distracted us from examining more important questions, such as asking researchers what they want to know when they conduct scientific research. Before we can improve our statistical inferences, we need to improve our statistical questions.},
 author = {Lakens, Dani{\"e}l},
 year = {2021},
 title = {The Practical Alternative to the p Value Is the Correctly Used p Value},
 keywords = {equivalence tests;null-hypothesis testing;p values;statistical inferences},
 pages = {639--648},
 volume = {16},
 number = {3},
 issn = {1745-6916},
 journal = {Perspectives on psychological science : a journal of the Association for Psychological Science},
 doi = {10.1177/1745691620958012},
 file = {1745691620958012:Attachments/1745691620958012.pdf:application/pdf}
}


