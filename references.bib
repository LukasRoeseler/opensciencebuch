 pages = {118--128},
 volume = {20},
 number = {2},
 issn = {1936-2293},
 journal = {Experimental and clinical psychopharmacology},
 doi = {10.1037/a0026623},
 file = {ContentServer (17):Attachments/ContentServer (17).pdf:application/pdf}
}


