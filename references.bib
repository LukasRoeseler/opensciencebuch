 issn = {1873-7943},
 journal = {Journal of behavior therapy and experimental psychiatry},
 doi = {10.1016/j.jbtep.2012.04.001},
 file = {braams{\_}2012{\_}-{\_}the{\_}effects{\_}of{\_}acceptance{\_}and{\_}suppression{\_}on{\_}anticipation{\_}and{\_}receipt{\_}of{\_}painful{\_}stimulation:Attachments/braams{\_}2012{\_}-{\_}the{\_}effects{\_}of{\_}acceptance{\_}and{\_}suppression{\_}on{\_}anticipation{\_}and{\_}receipt{\_}of{\_}painful{\_}stimulation.pdf:application/pdf}
